{
  "prettier.enable": true,
  "typescript.tsdk": "node_modules/typescript/lib",
  "volar.style.defaultLanguage": "postcss",
  "volar.tsPlugin": true,
  "volar.tsPluginStatus": false,
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
  },
  "files.associations": {
    "*.css": "postcss",
  },
  "editor.quickSuggestions": {
    "strings": true
  }
}
